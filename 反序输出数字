using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace reverse
{
    class Program
    {
        static void Main(string[] args)
        {
            String word = Console.ReadLine();
            for (int i = word.Length - 1; i > -1;i-- )
            {
                Console.Write(word[i]+" ");
            }
            Console.Read();
        }
    }
}
